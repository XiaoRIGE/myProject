{
  "name": "vue3-base-type",
  "version": "0.1.22",
  "description": "一个使用vue3+typescript 搭建的项目基础架构类型声明库",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "yarn script:check-empty && vue-cli-service serve --mode development",
    "serve:preview": "yarn script:check-empty && vue-cli-service serve --mode preview",
    "serve:product": "yarn script:check-empty && vue-cli-service serve --mode production",
    "build": "yarn build:product",
    "build:dev": "yarn script:check-empty && vue-cli-service build --mode development",
    "build:preview": "yarn script:check-empty && vue-cli-service build  --mode preview",
    "build:product": "yarn script:check-empty && vue-cli-service build --mode production",
    "build:analysis": "cross-env use_analyzer=true vue-cli-service build",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0 -s",
    "lint": "vue-cli-service lint",
    "docs": "yarn doc:request",
    "docs:request": "typedoc ./src/request/index.ts --out ./docs && yarn serve:docs",
    "script:check-empty": "node --trace-warnings --unhandled-rejections=strict -r ts-node/register -r ./tsconfig.json ./src/scripts/ProjectFolderEmptyChecker.ts",
    "script:check-version": "yarn yarn-check -u",
    "commit": "npx cz",
    "prepare": "husky install"
  },
  "main": "dist/index.js",
  "files": [
    "@types"
  ],
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@tinymce/tinymce-vue": "^4",
    "@vueuse/core": "^4.0.0-beta.16",
    "amap-js": "^2.3.0",
    "ant-design-vue": "^2.0.0-beta.9",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "echarts": "^5.1.2",
    "moment": "^2.27.0",
    "prettier-eslint": "^12.0.0",
    "qs": "^6.10.1",
    "sha1": "^1.1.1",
    "tinymce": "^5.9.2",
    "ts-md5": "^1.2.9",
    "ts-transformer-keys": "^0.4.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^3.0.0",
    "vue-clipboard3": "^1.0.1",
    "vue-composable": "^1.0.0-beta.10",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "vuex-persistedstate": "^3.1.0",
    "vxe-table": "^4.2.3",
    "xe-utils": "^3.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^5.2.4",
    "@types/node": "^15.0.2",
    "@types/sha1": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "chai": "^4.1.2",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "less": "^2.7.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "^1.19.1",
    "style-resources-loader": "^1.3.2",
    "stylelint": "^13.13.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "4.0.2",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "src/**/*.{jsx,txs,ts,js,json,vue,md}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue3",
    "typescript",
    "base"
  ],
  "license": "MIT",
  "types": "./@types/index.d.ts"
}
